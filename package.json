{
  "name": "@hexonet/semantic-release-github-npm-config",
  "version": "2.0.0",
  "description": "semantic-release shareable basic configuration for releasing on GitHub and npm",
  "main": ".releaserc.json",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint . --fix --config=./.eslintrc.js --ext .js,.md,.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hexonet/semantic-release-github-npm-config.git"
  },
  "keywords": [
    "semantic-release",
    "semantic-release-config",
    "github",
    "git",
    "release",
    "changelog",
    "npm"
  ],
  "author": "Kai Schwarz <kschwarz@hexonet.net>",
  "license": "MIT",
  "engines": {
    "node": ">=10.18.0"
  },
  "bugs": {
    "url": "https://github.com/hexonet/semantic-release-github-npm-config/issues"
  },
  "homepage": "https://github.com/hexonet/semantic-release-github-npm-config#readme",
  "dependencies": {
    "npm": ">=6.13.4",
    "@semantic-release/npm": "^7.0.3",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.4",
    "@semantic-release/release-notes-generator": "^9.0.1"
  },
  "peerDependencies": {
    "semantic-release": "^17.0.4"
  },
  "devDependencies": {
    "npm": ">=6.13.4",
    "semantic-release": "^17.0.4",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  }
}
